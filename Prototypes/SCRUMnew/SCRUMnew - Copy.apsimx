                        "        [EventSubscribe(\"StartOfSimulation\")]",
                        "        private void OnSimulationCommencing(object sender, EventArgs e)",
                        "        {",
                        "            InitialDay = true;",
                        "            StoverExpandedPosition = 0;",
                        "            StoverAppearedPosition = 0;",
                        "            ExpandedStoverNoYesterday = 0;",
                        "            AppearedStoverNoYesterday = 0;",
                        "            MostRecentlyExpandedStoverSize = 0;",
